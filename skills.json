---
layout: null
---
{
  {% for class in site.data.classes %}
   "{{ class.page }}": {
    "skills": [
    {% for skill in site.data[class.page].skills %}
      {% assign skill_data = site.data[class.page].skills | where: "slug", skill.slug | first %}
      {{ skill_data | jsonify }}{% if forloop.last == false %},{% endif %}
    {% endfor %}
    ],
    "non_stance_skills": [
    {% for skill in site.data[class.page].non_stance_skills %}
      "{{ skill.slug }}"{% if forloop.last == false %},{% endif %}
    {% endfor %}
    ]
  }{% if forloop.last == false %},{% endif %}
  {% endfor %}
}
